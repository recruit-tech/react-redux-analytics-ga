{
  "root": true,
  "extends": [
    "eslint-config-airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "globals": {
    "window": true
  },
  "parser": "babel-eslint",
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "import/prefer-default-export": "off",
    "dependencies/no-unresolved": "off"
  }
}